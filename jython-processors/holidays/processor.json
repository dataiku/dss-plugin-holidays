{
    "meta" : {
        "label" : "Holidays processor",
        "description" : "Step to flag UK holidays from a date column",
        "icon" : "icon-calendar"
    },
    "type": "PythonUDF",
    "mode" : "ROW",
    "envSelection": {
        "envMode": "USE_BUILTIN_MODE"
    },
    "useKernel": true,
    "usePythonUnicode": true,
    "params": [
        {
            "name": "column",
            "label" : "Input column",
            "type": "COLUMN",
            "columnRole":"input_dataset",
            "mandatory" : true
        },
        {
            "name": "country",
            "label": "Country",
            "type": "SELECT",
            "selectChoices": [
                { "value": "GB", "label": "United Kingdom"}
            ],
            "defaultValue": "GB"
        },
        {
            "name": "extract_name",
            "label": "Extract holiday name",
            "type": "BOOLEAN",
            "description": ""
        }
    ]
}
